<%= form_with model: address do |form| %>
  <div>
    <%= form.label :street %><br>
    <%= form.text_field :street %>
    <% address.errors.full_messages_for(:street).each do |message| %>
      <div><%= message %></div>
    <% end %>
  </div>

  <div>
    <%= form.label :city %><br>
    <%= form.text_field :city %><br>
    <% address.errors.full_messages_for(:city).each do |message| %>
      <div><%= message %></div>
    <% end %>
  </div>

  <div>
    <%= form.label :zip_code %><br>
    <%= form.text_field :zip_code %><br>
    <% address.errors.full_messages_for(:zip_code).each do |message| %>
      <div><%= message %></div>
    <% end %>
  </div>

  <div>
    <%= form.label :state %><br>
    <%= form.text_field :state %><br>
    <% address.errors.full_messages_for(:state).each do |message| %>
      <div><%= message %></div>
    <% end %>
  </div>

  <div>
    <%= form.label :country %><br>
    <%= form.text_field :country %><br>
    <% address.errors.full_messages_for(:country).each do |message| %>
      <div><%= message %></div>
    <% end %>
  </div>

  <div>
    <%= form.submit %>
  </div>
<% end %>
